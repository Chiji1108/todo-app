    "@remix-run/react": "^2.7.2",
    "framer-motion": "^11.0.6",
    "isbot": "^5.1.0",
    "lucide-react": "^0.337.0",
    "openapi-fetch": "^0.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240222.0",
    "@pandacss/dev": "^0.32.0",
    "@park-ui/panda-preset": "^0.35.2",
